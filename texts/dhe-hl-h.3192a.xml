         <div type="letter">
            <pb n="1" facs="uoedh_hl_h.3192a_1.jpg"/>
            <opener>
               <address rend="letterhead">
                  <addrLine>
                     <del>TEL.:</del>
                  </addrLine>
                  <addrLine>
                     <del>HORNSEY</del>
                  </addrLine>
